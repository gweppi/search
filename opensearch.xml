<OpenSearchDescription
  xmlns="http://a9.com/-/spec/opensearch/1.1/"
  xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Gwep Search</ShortName>
  <Description>The Gwep Search Engine using DuckDuckGo Bangs and Google Completions.</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16" type="image/x-icon">https://search.gwep.dev/favicon.ico</Image>
  <Url type="text/html" template="https://search.gwep.dev?q={searchTerms}"/>
  <Url type="application/x-suggestions+json" template="https://www.google.com/complete/search?q={searchTerms}&amp;client=chrome"/>
  <Url type="application/json" rel="suggestions" template="http://my_site/suggest?q={searchTerms}" />
</OpenSearchDescription>